{
  "name": "@baleyui/sdk",
  "version": "0.1.0",
  "description": "Official JavaScript/TypeScript SDK for BaleyUI - Execute AI flows and blocks programmatically",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "baleyui",
    "ai",
    "sdk",
    "api",
    "flows",
    "blocks",
    "llm",
    "typescript"
  ],
  "author": "BaleyUI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/baleyui/baleyui.git",
    "directory": "packages/sdk"
  },
  "homepage": "https://baleyui.com/docs/sdk",
  "bugs": {
    "url": "https://github.com/baleyui/baleyui/issues"
  },
  "dependencies": {
    "@baleybots/core": "^0.0.1-alpha.95",
    "@baleybots/tools": "^0.0.1-alpha.95"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.16",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.16"
  },
  "peerDependencies": {
    "typescript": ">=4.7.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
